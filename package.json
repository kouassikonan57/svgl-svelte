{
	"name": "@selemondev/svgl-svelte",
	"version": "0.0.3",
	"description": "An optimized package with SVG logos to be used as Svelte components âœ¨",
	"author": "Selemondev",
	"private": false,
	"license": "MIT",
	"packageManager": "pnpm@10.6.5",
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"generate:components": "pnpm dlx esno scripts/index.ts && pnpm format",
		"generate:release": "pnpm dlx changelogen@latest --release && npm publish",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint . && prettier --write ."
	},
	"files": [
		"dist",
		"!dist/components/*.d.ts",
		"!dist/types/*.d.ts",
		"!dist/utils/*.d.ts",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/node": "^22.15.19",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"fs-extra": "^11.3.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	},
	"keywords": [
		"svgl",
		"svgl-svelte",
		"logos",
		"svg",
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"publishConfig": {
		"access": "public"
	}
}
